{
  "manifest_version": 3,
  "name": "vsearch-chrome",
  "description": "Chrome Extension built with React",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    }
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "tabs",
    "https://*.youtube.com/*",
    "https://*.youtu.be/*",
    "https://*.svc.*.pinecone.io/*",
    "webRequest",
    "storage",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "https://*.youtube.com/*",
    "https://*.youtu.be/*",
    "https://*.svc.*.pinecone.io/*"
  ]
}